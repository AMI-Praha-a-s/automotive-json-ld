{
    "name": "ami-praha/automotive-json-ld",
    "description": "A Laravel package to help to generate json-ld scripts",
    "keywords": [
        "ami-praha",
        "laravel",
        "automotive-json-ld"
    ],
    "homepage": "https://github.com/AMI-Praha-a-s/automotive-json-ld",
    "license": "MIT",
    "authors": [
        {
            "name": "David Tomsik",
            "email": "david.tomsik@bestfit.cz",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4||^8.1||^8.2||^8.3||^8.4",
        "illuminate/console": "*",
        "illuminate/contracts": "^10.0||^11.0||^12.0",
        "illuminate/support": "*",
        "spatie/schema-org": "^3.23"
    },
    "autoload": {
        "psr-4": {
            "AmiPraha\\AutomotiveJsonLd\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "AmiPraha\\AutomotiveJsonLd\\AutomotiveJsonLdServiceProvider"
            ],
            "aliases": {
                "AutomotiveJsonLd": "AmiPraha\\AutomotiveJsonLd\\Facades\\AutomotiveJsonLd"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
